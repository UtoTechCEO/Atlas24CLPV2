---
// pages/umzugsfirma-zuerich.astro

export const prerender = true;

import Baselayout from "../layouts/Baselayout.astro";
import "../styles/global.css";
import Header_Umzugsfirma_Zuerich from "../components/Header_Umzugsfirma_Zuerich.astro";
import Hero_Umzugsfirma_Zuerich from "../components/Hero_Umzugsfirma_Zuerich.astro";
